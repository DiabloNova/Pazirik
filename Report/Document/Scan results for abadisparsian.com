@charset "utf-8";

.dropotron a, .dropotron span { text-decoration: none; }

#content header, #sidebar header { margin-bottom: 2em; }

body, input, select, textarea { line-height: 1.65em; font-size: 11pt; }

header h2 { line-height: 1.4em; font-size: 1.5em; }

ul.social { position: absolute; right: 0px; }

ul.social li:last-child { padding-right: 0px; }

#header .container { position: relative; padding: 3em 0px 1em; }

#nav { position: absolute; }

#nav { right: 0px; top: 1em; }

#nav > ul > li { float: left; padding: 0px 1.2em; }

#nav > ul > li:last-child { padding-right: 0px; }

#nav > ul > li > a, #nav > ul > li > span { display: block; letter-spacing: 0.06em; text-decoration: none; font-weight: 600; font-size: 1.1em; outline: 0px; color: rgba(255, 255, 255, 0.65); transition: color 0.25s ease-in-out; }

#nav li:hover a, #nav li:hover span { color: rgb(255, 255, 255); }

#nav li.active a { padding-bottom: 5px; border-bottom: 2px solid rgba(255, 255, 255, 0.75); color: rgb(255, 255, 255); }

#nav > ul > li > ul { display: none; }

.dropotron { top: 0px; text-align: left; min-width: 13em; background: rgb(41, 44, 49); line-height: 1em; padding: 1em 0px; border-radius: 8px; }

#slider .icon, #sponsor-footer { text-align: center; }

.dropotron a, .dropotron span { display: block; padding: 0.9em 1.7em; border-bottom: 0px; font-size: 0.9em; color: rgb(218, 219, 222); }

.dropotron li:first-child > a, .dropotron li:first-child > span { border-top: 0px; }

.dropotron li.active > a, .dropotron li.active > span, .dropotron li:hover > a, .dropotron li:hover > span { color: rgb(255, 255, 255); }

.dropotron.level-0 { font-size: 1.1em; margin-top: 1.7em; border-top-left-radius: 0px; border-top-right-radius: 0px; }

body.homepage .dropotron.level-0 { margin-top: 1.5em; border-top-left-radius: 8px; border-top-right-radius: 8px; }

.dropotron.level-0::before { content: ""; position: absolute; top: -1.2em; left: 5.5em; width: 1.3em; height: 1.3em; border-bottom: 0.8em solid rgb(44, 44, 44); border-left: 0.8em solid transparent; border-right: 0.8em solid transparent; }

#box1, #box2 { height: 422px; }

#box2::after, #box3::after, #box3::before { content: ""; display: block; width: 100%; height: 100%; top: 0px; position: absolute; }

