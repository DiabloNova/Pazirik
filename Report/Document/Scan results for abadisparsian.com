.bubble-top { display: block; outline: 0px; margin: 0px auto 2em; width: 60px; height: 60px; background: rgb(58, 113, 216); border-radius: 50%; text-align: center; line-height: 60px; font-size: 1.4em; font-weight: 700; color: rgb(255, 255, 255); transition: background-color 0.25s ease-in-out; }

#box3::before, #featured .pennant, .alert, .icon::before { display: inline-block; }

.bubble-top::after { content: ""; position: absolute; width: 0px; height: 0px; border-left: 10px solid transparent; border-right: 10px solid transparent; border-bottom: 15px solid rgb(58, 113, 216); left: 35%; top: -10px; transition: border-color 0.25s ease-in-out; }

.bubble-top:hover::after { border-bottom-color: rgb(74, 129, 232); }

.bubble-top:active::after { border-bottom-color: rgb(42, 97, 200); }

.push-top { margin-top: 10px; }

.text-right { text-align: right !important; }

.icon::before { font-family: FontAwesome; font-size: 1.25em; font-style: normal; font-weight: 400; line-height: 1; -webkit-font-smoothing: antialiased; }

#header { background: rgb(58, 113, 216); }

#header.red { background: rgb(219, 30, 30); }

#header.orange { background: rgb(229, 115, 34); }

#header.yellow { background: rgb(255, 210, 66); }

#header.green { background: rgb(43, 145, 0); }

#header.lightgreen { background: rgb(52, 175, 0); }

#header.grey { background: rgb(131, 131, 131); }

#banner header span { display: block; color: rgba(255, 255, 255, 0.5); }

#main { background: rgb(255, 255, 255); }

div .border { background: rgb(240, 248, 255); padding: 0px 20px 20px 10px; border: 2px solid rgb(214, 235, 255); }

#footer { border-top: 1px solid rgb(230, 231, 231); }

#copyright .container { position: relative; border-top: 1px solid rgb(202, 205, 210); border-bottom: 1px solid rgb(202, 205, 210); }

#featured { text-align: center; }

#featured .pennant { color: rgb(58, 113, 216); cursor: default; }

.reportSection { border: 2px solid rgb(168, 192, 193); background: rgb(240, 248, 255); margin-bottom: 20px; text-align: center; }

.reportTitle { text-align: left; font: 700 23px / 28px Arial, Helvetica, sans-serif; color: rgb(105, 108, 113); padding: 16px; background: rgb(221, 238, 255); border-bottom: 1px solid rgb(198, 210, 212); }

.tableCell, .tableLabel { padding: 3px 10px; overflow-wrap: break-word; }

.reportBody { margin: 20px 15px; }

table.reportTable { margin: 0px 10px 0px 0px; padding: 0px; font-size: 13px; line-height: 20px; table-layout: fixed; }

.tableLabel { text-align: left; font-size: 13px; font-weight: 700; color: rgb(68, 68, 68); border-bottom: 1px solid rgb(225, 225, 225); vertical-align: middle; }

.tableCell, .tableCellRecent { text-align: left; color: rgb(102, 102, 102); font-size: 13px; border-bottom: 1px solid rgb(225, 225, 225); vertical-align: middle; }

.tableCell { word-break: break-word; hyphens: auto; }

.tableCellRecent { padding: 3px; hyphens: auto; }

.score, .score_red { text-align: center; }

col.col1front { width: 90%; }

col.col2front { width: 10%; }

col.col1front50, col.col2front50 { width: 50%; }

col.col1 { width: 20%; }

col.col2 { width: 80%; }

.push-left { padding-left: 80px; }

input.checkbox { vertical-align: text-top; }

.score { padding: 0px 20px; margin-bottom: 0px; float: left; width: 150px; }

.score_red { font-family: Arial, Helvetica, sans-serif; margin: auto; width: 130px; height: 130px; font-size: 90px; line-height: 135px; font-weight: 700; background-color: rgb(219, 30, 30); border-radius: 12px; border: 2px solid rgb(173, 27, 27); }

.score_orange, .score_yellow { font-family: Arial, Helvetica, sans-serif; text-align: center; margin: auto; width: 130px; height: 130px; font-size: 90px; line-height: 135px; font-weight: 700; color: rgb(255, 255, 255); border-radius: 12px; }

.score_orange { background-color: rgb(229, 115, 34); border: 2px solid rgb(219, 97, 7); }

.score_yellow { background-color: rgb(255, 210, 66); border: 2px solid rgb(255, 192, 0); }

.score_green, .score_lightgreen { border-radius: 12px; font-family: Arial, Helvetica, sans-serif; text-align: center; margin: auto; width: 130px; height: 130px; font-size: 90px; line-height: 135px; font-weight: 700; color: rgb(255, 255, 255); }

.score_green { background-color: rgb(43, 145, 0); border: 2px solid rgb(37, 121, 0); }

.score_lightgreen { background-color: rgb(52, 175, 0); border: 2px solid rgb(48, 157, 0); }

.score_grey { font-family: Arial, Helvetica, sans-serif; text-align: center; margin: auto; width: 130px; height: 130px; font-size: 90px; line-height: 135px; font-weight: 700; color: rgb(255, 255, 255); background-color: rgb(131, 131, 131); border-radius: 12px; border: 2px solid rgb(92, 92, 92); }

.table_red { color: red; font-weight: 700; }

.table_orange { color: orange; font-weight: 700; }

.table_yellow { color: rgb(255, 210, 66); font-weight: 700; }

.table_green { color: rgb(65, 168, 50); font-weight: 700; }

.table_lightgreen { color: rgb(78, 200, 61); font-weight: 700; }

.table_grey { color: grey; font-weight: 700; }

.table_blue { color: rgb(17, 108, 255); font-weight: 700; }

li.pill { color: rgb(255, 255, 255); border-radius: 8px; padding: 1px 6px; margin: 1px 0px; }

li.pill-green { background: green; color: rgb(255, 255, 255); }

li.pill-red { background: red; color: rgb(255, 255, 255); }

li.pill-orange { background: orange; color: rgb(255, 255, 255); }

.pill i { margin: 0px 4px 0px 0px; }

#box1 { background: url("images/shadow-bg.png") center bottom no-repeat; z-index: 1; }

#box2, #box3 { position: absolute; bottom: 0px; }

#box3::before { width: 100%; left: -4.55em; bottom: -2px; z-index: 5; }

#slider, #slider .viewer, .alert { position: relative; }

#slider { background: url("images/monitor-bg.png") center top no-repeat; }

#slider .viewer::before { display: block; width: 100%; top: 0px; background: url("images/monitor-shine.png") center top no-repeat; z-index: 1; }

.sponsor-image { max-height: 80px; width: 100%; display: block; height: auto; }

.alert { padding: 0.75rem 1.25rem; margin-bottom: 1rem; border: 1px solid transparent; border-radius: 0.25rem; }

.alert-success { color: rgb(21, 87, 36); background-color: rgb(212, 237, 218); border-color: rgb(195, 230, 203); }

.alert-danger { color: rgb(114, 28, 36); background-color: rgb(248, 215, 218); border-color: rgb(245, 198, 203); }

#logo { padding-top: 3px; display: flex; justify-content: left; }
